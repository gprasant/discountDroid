<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <com.aurasma.aurasma.trackingar.CameraPreview
        android:id="@+id/aurasma_cameraPreview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.aurasma.aurasma.ui.GLContainer
        android:id="@+id/aurasma_GLSurface"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="true"
        android:focusableInTouchMode="true" />

    <com.aurasma.aurasma.ui.TranslatedWebView
        android:id="@+id/aurasma_webEmbed"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/aurasma_autoFocusHint"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="top|center"
        android:paddingTop="10dip"
        android:shadowColor="#0106000e"
        android:text="@string/aurasma_AutoFocusHintText"
        android:textSize="20sp"
        android:visibility="invisible" />

</merge>